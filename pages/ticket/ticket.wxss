  flex-direction: column;
  box-shadow:0rpx 8rpx 20rpx 0rpx rgba(0, 0, 0, .04);
}
.main .swiper .cercleBox{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  background-color: #FAFAFA;
  position: absolute;
}
.main .swiper .leftOne{

  top: 16.5%;
