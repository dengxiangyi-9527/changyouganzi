/* pages/CustomTour//pages/order/index.wxss */
page{
  width: 750rpx;
  min-height: 100%;
  background-color: #F2F3F7;
}
.wrap{
  width: 750rpx;
  padding: 29rpx 20rpx 230rpx 20rpx;
  box-sizing: border-box;
}
.carBox{
  width: 100%;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  background-color: #FFF;
  border-radius: 24rpx;
}
.carInfo{
  padding-bottom: 41rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #EDEDED;
  display: flex;
  align-items: center;
}
.carImg{
  width: 240rpx;
  height: 140rpx;
  vertical-align: top;
}
.carText{
  margin-left: 16rpx;
}
.carName{
  font-size: 32rpx;
  line-height: 44rpx;
  color: #333;
  margin-top: 8rpx;
  font-weight: bold;
}
.info{
  font-size: 24rpx;
  line-height: 36rpx;
  color: #333;
  margin-top: 8rpx;
}
.timeLoc{
  padding-top: 20rpx;
  box-sizing: border-box;
}
.timeBox{
  display: flex;
  align-items: center;
}
.timelocTitle{
  width: 70rpx;
  height: 50rpx;
  background-color: #E9F8F6;
  color: #37C776;
  font-size: 24rpx;
  line-height: 36rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.time{
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}
.date{
  font-size: 30rpx;
  line-height: 42rpx;
  color: #333;
  font-weight: bold;
}
.dayNum{
  margin: 0 44rpx;
  display: flex;
  align-items: center;
}
.dayLine{
  width: 20rpx;
  height: 3rpx;
  background-color: #AEAEAE;
}
.day{
  padding:4rpx 20rpx;
  box-sizing: border-box;
  line-height: 30rpx;
  font-size: 20rpx;
  color: #333;
  background-color: #F7F7F7;
  border: 1rpx solid #D4D4D4;
  border-radius: 19rpx;
}
.locBox{
  margin-top: 20rpx;
  display: flex;
}
.loc{
  font-size: 28rpx;
  line-height: 40rpx;
  color: #333;
  margin-left: 20rpx;
}
.wrap .userInfo{
  margin-top: 24rpx;
  width: 100%;
  padding: 41rpx 30rpx 20rpx 30rpx;
  box-sizing: border-box;
  background-color: #FFF;
  border-radius: 24rpx;
  padding-top: 41rpx;
}
.wrap .userInfo .userTitle{
  font-size: 36rpx;
  color: #333;
  line-height: 50rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.wrap .userInfo .userInfoBox .infoBox{
  display: flex;
  border-bottom: 1rpx solid #EDEDED;
  align-items: center;
  padding: 30rpx 0;
  box-sizing: border-box;
}
.wrap .userInfo .userInfoBox .userInfoList{
  width: 100%;
}
.wrap .userInfo .userInfoBox .userInfoList .userInfoItem{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #EDEDED;
}

.wrap .userInfo .userInfoBox .userInfoList .userInfoItem .itemInfo{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0rpx 16rpx 0rpx;
  box-sizing: border-box;
}
.wrap .userInfo .userInfoBox .userInfoList .userInfoItem .itemInfo .itemUser{
  width: 100%;
  display: flex;
  align-items: center;
}
.wrap .userInfo .userInfoBox .userInfoList .userInfoItem .itemInfo .itemUser .itemUserInfo{
  margin-left: 30rpx;
}
.wrap .userInfo .userInfoBox .userInfoList .userInfoItem .itemInfo .itemUser .itemUserInfo .itemUserName{
  font-size: 32rpx;
  line-height: 44rpx;
  color: #333;
  font-weight:700;
}
.wrap .userInfo .userInfoBox .userInfoList .userInfoItem .itemInfo .itemUser .itemUserInfo .itemUserid{
  font-size: 26rpx;
  line-height: 38rpx;
  color: #999;
}
.wrap .userInfo .userInfoBox .userInfoList .userInfoItem .itemInfo .itemUser image{
  width: 34rpx;
  height: 34rpx;
  vertical-align: top;
}
.wrap .userInfo .userInfoBox .userInfoList .userInfoItem .scrolview{
  display: flex;
  height: 100%;
  overflow: hidden;
}
.wrap .userInfo .userInfoBox .userInfoList .userInfoItem .scrollBox{
  width: 184rpx;
  display: flex;
  justify-content: flex-end;
  width: 100%;
  margin-left: 100rpx;
}
.wrap .userInfo .userInfoBox .userInfoList .userInfoItem .itemclose{
  height: 100%;
  padding: 46rpx 18rpx;
  box-sizing: border-box;
  background-color: #FE4B3C;
  color: #fff;
  font-size: 28rpx;
  line-height: 40rpx;
}
.wrap .userInfo .userInfoBox .userInfoList .userInfoItem .closeBox{
  flex: 1;
  overflow: hidden;
}
.wrap .userInfo .userInfoBox .infoBox:nth-last-child(1){
  border-bottom: none;
}
.wrap .userInfo .userInfoBox .infoBox .infoTitle{
  display: flex;
  align-items: center;
}
.wrap .userInfo .userInfoBox .infoBox .infoTitle image{
  width: 40rpx;
  height: 40rpx;
}
.wrap .userInfo .userInfoBox .infoBox .infoTitle .titleText{
  font-size: 32rpx;
  line-height: 44rpx;
  color: #09AFFF;
  margin-left: 24rpx;
}
.wrap .userInfo .userInfoBox .infoBox .inputBox{
  margin-left: 48rpx;
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
} 
.wrap .userInfo .userInfoBox .infoBox .inputBox .palce{
  font-size: 32rpx;
  color: #D1D1D1;
  line-height: 44rpx;
  font-weight: 500;
}
.wrap .userInfo .userInfoBox .infoBox .inputBox .inputText{
  color: #333333;
  line-height: 44rpx;
  font-size: 32rpx;
  font-weight: 600;
}
.wrap .userInfo .userInfoBox .infoBox .perseonNum{
  display: flex;
  justify-content: space-between;
}
.wrap .userInfo .userInfoBox .infoBox .line{
  display: flex;
  width: 1rpx;
  background-color: #D1D1D1;
  height: 30rpx;
}
.wrap .userInfo .userInfoBox .infoBox .perseonNum .numTitle{
  line-height: 44rpx;
  font-size: 32rpx;
  color: #09AFFF;
  margin-right: 80rpx;
  margin-left: 24rpx;
}
.wrap .userInfo .userInfoBox  .personBox{
  display: flex;
  justify-content: space-between;
}
.wrap .userInfo .userInfoBox .infoBox .perseonNum .numBox{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.wrap .userInfo .userInfoBox .infoBox .perseonNum .numBox .btnBox{
  width: 32rpx;
  height: 32rpx;
}
.wrap .userInfo .userInfoBox .infoBox .perseonNum .numBox .btnBox image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: top;
}
.wrap .userInfo .userInfoBox .infoBox .perseonNum .numBox .num{
  font-size: 30rpx;
  line-height: 42rpx;
  color: #333;
  margin: 0 16rpx;
}
.wrap .userInfo .userInfoBox .infoBox .addUserInfo{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
}
.wrap .userInfo .userInfoBox .infoBox .addUserInfo image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: top;
}
.wrap .userInfo .userInfoBox .infoBox .addUserInfo .addUserText{
  font-size: 32rpx;
  font-weight: bold;
  line-height: 44rpx;
  color: #09AFFF;
  margin-left: 12rpx;
}
.erroBox{
  margin-top: 24rpx;
  padding: 40rpx 30rpx 30rpx 30rpx;
  box-sizing: border-box;
  background-color: #FFF;
  border-radius: 24rpx;
}
.indexTitle{
  font-size: 36rpx;
  line-height: 50rpx;
  color: #333;
  font-weight: bold;
}
.title{
  font-size: 28rpx;
  line-height: 40rpx;
  color: #333;
  margin-top: 20rpx;
}
.erroText{
  font-size: 24rpx;
  color: #999;
  line-height: 36rpx;
  margin-top: 4rpx;
}
.electronicInvoice{
  margin-top: 24rpx;
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  background-color: #FFF;
  display: flex;
  border-radius: 24rpx;
  justify-content: space-between;
}
.invoiceTitle{
  font-size: 32rpx;
  line-height: 44rpx;
  color: #333;
  font-weight: bold;
}
.invoiceIcon{
  display: flex;
  align-items: center;
}
.iconText{
  font-size: 32rpx;
  line-height: 44rpx;
  color: #333;
}
.invoiceIcon image{
  width: 24rpx;
  height: 24rpx;
  vertical-align: top;
}
.prompt {
  width: 100%;
  margin-top: 24rpx;
  border-radius: 16rpx;
}
.prompt .tit{
  color: #3E4B6E;
  font-size: 26rpx;
  line-height: 38rpx;
}
 .prompt .content {
  padding-bottom: 16rpx;
}

 .prompt .content view {
  color: #3E4B6E;
  font-size: 26rpx;
  line-height: 38rpx;
} 
 .prompt .content view text {
  color: #09afff;
  font-size: 26rpx;
  line-height: 38rpx;
}
.protocol {
  display: flex;
  align-items: center;
  font-size: 26rpx;
}
 .protocol .protocol_content {
  color: #09AFFF;
  font-size: 23rpx;
}
 .protocol .read {
  color: #3E4B6E;
} 
.toPay{
  width: 750rpx;
  background-color: #FFF;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
  z-index: 999999;
  padding-bottom: 68rpx;
  box-sizing: border-box;
  box-shadow: 0 -2rpx 20rpx 0 rgba(91, 99, 108, 0.05);
}
.toPay .BottompriceBox{
  display: flex;
}
.toPay .allPrice{
  font-size: 46rpx;
  line-height: 58rpx;
  color: #FE4B3C;
  margin-bottom: 12rpx;
  margin-left: 30rpx;
}
.toPay .title{
  display: flex;
  align-items: center;
  margin-top: 0;
  margin-bottom: 20rpx;
  margin-left: 30rpx;
}
.toPay .title .title_1{
  font-size: 28rpx;
  line-height: 40rpx;
  color: #999;
}
.toPay .title .title_2{
  width: 18rpx;
  height: 12rpx;
}
.toPay .title .title_2 image{
  vertical-align: top;
  width: 100%;
  height: 100%;
  margin-left: 8rpx;
}
.toPay .btnBox{
  padding: 22rpx 62rpx;
  background-color: #09AFFF;
  border-radius: 50rpx;
  display: flex;
  align-items: center;
  text-align: center;
  color: #FFF;
  font-size: 32rpx;
  line-height: 44rpx;
  font-weight: 600;
  margin-right: 30rpx;
  margin-top: 14rpx;
}
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  bottom: 268rpx;
  background-color: rgba(0, 0, 0, .3);
  box-sizing: border-box;
  z-index: 9999;
}
.mask .maskBox{
  position: fixed;
  bottom: 168rpx;
  width: 100%;
  background-color: #FAFAFA;
  z-index: 999999999;
}
.mask .maskBox .boxTitle{
  width: 100%;
  color: #333;
  line-height: 50rpx;
  padding: 30rpx 0 30rpx 30rpx;
  font-size: 32rpx;
  font-weight: 540;
  box-sizing: border-box;
}
.mask .maskBox .boxNum{
  border-top: 1rpx solid #EDEDED;
  box-sizing: border-box;
  width: 100%;
  padding: 30rpx;
  display: flex;
  justify-content: space-between;
}
.mask .maskBox .boxNum .Numtext{
  font-size: 32rpx;
  line-height: 44rpx;
  color:#333;
  font-weight: 520;
}