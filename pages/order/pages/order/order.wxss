
page{
  width: 750rpx;
  height: 100%;
  background-color: #F2F3F7;
}
.wrap{
  width: 750rpx;
  background-color: #F2F3F7;
  padding-top: 24rpx;
}
.imgBox{
  background-color: #FFF;
  padding: 19rpx 20rpx 25rpx 27rpx;
  box-sizing: border-box;
  display: flex;
  align-content: space-between;
  flex-wrap: wrap;
}
.imgBox .tabBox {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 15rpx 36rpx;
}

.imgBox .tabBox .img{
  width: 88rpx;
  height: 88rpx;
  margin-bottom: 16rpx;
}
.imgBox .tabBox .img image{
  width: 88rpx;
  height: 88rpx;
  vertical-align: top;
}
.imgBox .tabBox .text{
  font-size: 24rpx;
  font-weight: bold;
  line-height: 36rpx;
}
.textBox{
  background-color: #FFF;
  width: 750rpx;
  padding: 24rpx 0rpx 24rpx 40rpx;
  box-sizing: border-box;
  line-height: 44rpx;
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
  margin-top: 24rpx;
}
.orderList{
  width: 750rpx;
  margin-top: 40rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  padding-bottom: 168rpx;
}
.orderList .list_item {
  width: 690rpx;
  background-color: #fff;
  box-sizing: border-box;
  padding: 0 30rpx;
  margin-bottom: 24rpx;
  border-radius: 16rpx;
  padding-bottom: 30rpx;
  box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.04);
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}

.orderList .list_item:nth-child(1) {
  margin-top: 24rpx;
}

.orderList .list_item .item_title {
  width: 100%;
  height: 92rpx;
  border-bottom: 1rpx solid #EDEDED;
  overflow: hidden;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.orderList .list_item .item_title .title_left {
  display: flex;
}

.orderList .list_item .item_title .title_left .title_img {
  width: 44rpx;
  height: 44rpx;
  margin-right: 12rpx;
}

.orderList .list_item .item_title .title_left .title_img image {
  width: 100%;
  height: 100%;
}

.orderList .list_item .item_title .title_left .title_text {
  font-size: 30rpx;
  color: #333;
  font-weight: 600;
}

.orderList .list_item .item_title .title_right {
  height: 38rpx;
  font-size: 26rpx;
  color: #FE4B3B;
}

.orderList .list_item .item_main {
  width: 100%;
  box-sizing: border-box;
  padding-top: 24rpx;
  display: flex;
  position: relative;

}

.orderList .list_item .item_main .main_img {
  width: 170rpx;
  height: 100rpx;
}

.orderList .list_item .item_main .main_img image {
  width: 100%;
  height: 100%;
}

.orderList .list_item .item_main .main_text {
  flex: 1;
  margin-left: 10rpx;
  font-size: 26rpx;
  color: #666;
  line-height: 38rpx;
}

.orderList .list_item .item_main .main_text .text_title {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: #333333;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}

.orderList .list_item .item_main .main_text .text_title .title_dian {
  width: 12rpx;
  height: 12rpx;
  background-color: #D3D3D3;
  border-radius: 50%;
  display: inline-block;
  margin-right: 20rpx;
}

.orderList .list_item .item_main .main_text .text_person {
  margin-top: 8rpx;
  margin-bottom: 8rpx;
}

.orderList .list_item .item_main .main_text .shu {
  width: 2rpx;
  height: 20rpx;
  background-color: #EDEDED;
  margin-left: 5rpx;
}

.orderList .list_item .item_main .main_text .start_location,
.orderList .list_item .item_main .main_text .end_location {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #666666;
}

.orderList .list_item .item_main .main_text .start_location .dian,
.orderList .list_item .item_main .main_text .end_location .dian {
  width: 12rpx;
  height: 12rpx;
  background-color: #09AFFF;
  border-radius: 50%;
  display: inline-block;
  margin-right: 20rpx;
}

.orderList .list_item .item_main .main_text .end_location .dian {
  background-color: #24BFB0;
}

.orderList .list_item .item_main .text_price {
  position: absolute;
  bottom: 0;
  right: 0;
  z-index: 999;
  font-size: 30rpx;
  color: #333333;
  font-weight: 600;
}
.orderList .list_item  .btnBox{
  margin-top: 26rpx;
}

.orderList .list_item .item_main .main_text .text_person .person_tab {
  height: 38rpx;
  display: flex;
}

.allBox .allList .list_item .item_main .main_text .text_person .person_tab .tab_box {
  margin-right: 25rpx;
  display: flex;
  align-items: center;
}

.allBox .allList .list_item .item_main .main_text .text_person .person_tab .tab_box .person_tab_img {
  width: 30rpx;
  height: 30rpx;
  background-color: #24BFB0;
  color: #fff;
  font-size: 20rpx;
  line-height: 30rpx;
  text-align: center;
  border-radius: 50%;
  margin-right: 4rpx;

}

.allBox .allList .list_item .item_main .main_text .text_person .person_tab .tab_box .person_tab_imgR {
  width: 30rpx;
  height: 30rpx;
  background-color: #FE8B32;
  color: #fff;
  font-size: 20rpx;
  line-height: 30rpx;
  text-align: center;
  border-radius: 50%;
  margin-right: 4rpx;
}

.allBox .allList .list_item .item_main .main_text .text_person .person_tab .tab_box .person_tab_text {
  font-size: 26rpx;
  color: #666;
  line-height: 38rpx;
}

.allBox .allList .list_item .item_main .main_text .btn_box {
  display: flex;
  margin-top: 24rpx;
  justify-content: flex-end;
  margin-bottom: 24rpx;
}

.allBox .allList .list_item .item_main .main_text .main_btn {
  height: 56rpx;
  border: 2rpx solid #A9E1FF;
  text-align: center;
  line-height: 54rpx;
  color: #18AFFF;
  font-size: 28rpx;
  border-radius: 27.5rpx;
  padding: 0 24rpx;
}

.allBox .allList .list_item .item_main .main_text .main_btn:nth-child(1) {
  margin-right: 24rpx;
}

.allBox .allList .list_item .go_pay {
  background-color: #18AFFF;
  color: #FFF;
  font-size: 28rpx;
  line-height: 40rpx;
  padding: 7rpx 24rpx;
  box-sizing: border-box;
  border-radius: 27.5rpx;
  margin-top: 24rpx;
  margin: 0 7rpx;
}

.allBox .allList .list_item .go_pay::after {
  border: none;
}

.allBox .allList .list_item .btnBox {
  margin-top: 26rpx;
}